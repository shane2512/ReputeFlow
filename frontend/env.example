# Get your project ID from https://cloud.walletconnect.com
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Optional: Enable testnets
NEXT_PUBLIC_ENABLE_TESTNETS=true

# Agentverse Agent HTTP Endpoints
# Get these from your Agentverse dashboard after deploying agents
CLIENT_AGENT_URL=https://agentverse.ai/v1/agents/your-client-agent-id/submit
FREELANCER_AGENT_URL=https://agentverse.ai/v1/agents/your-freelancer-agent-id/submit

# OpenRouter API Key (fallback if agents are unavailable)
# Get from https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Site URL for OpenRouter attribution
NEXT_PUBLIC_SITE_URL=https://your-site.vercel.app
